{
  "hosting": {
    "public": "build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/studio/**",
        "destination": "/studio/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}

